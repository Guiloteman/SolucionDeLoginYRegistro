
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <link href="~/Content/css/style.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.6.0/css/all.min.css" integrity="sha512-Kc323vGBEqzTmouAECnVceyQqyqdsSiqLQISBL29aUW4U/M7pSPA/gEUZQqv1cwx4OnYxTxve5UMg5GT6L4JJg==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    
</head>
<body>

    <header class="header">
        <a href="./index.html" class="logo">Dirección General de Catastro</a>
        @*<img src="https://pbs.twimg.com/profile_images/1083007523983163394/ahsIgsyK_400x400.jpg" alt="logo">*@
        <img src="~/Imagen/images.png" alt="logo">
        <a href="./index.html" class="logo">Provincia de Tucumán</a>
    </header>

    <form class="form" action="@Url.Action("CargarImagen", "PresentarCarpeta")" method="post">
        <h1 class="title">Presentar Carpeta</h1>
        <label>
            <i class="fa-solid fa-user"> - Correo</i>
            <input name="Correo" placeholder="correo" type="email" id="username" required>
        </label>

        <label>
            <i class="fa-solid fa-dollar-sign"> - Timbrado</i>
            <input name="Timbrado" placeholder="timbrado" type="file" id="timbrado" required>
        </label>

        <label>
            <i class="fa-solid fa-file"> - Formulario</i>
            <input name="Formulario" placeholder="formulario" type="file" id="formulario" required>
        </label>

        <label>
            <i class="fa-solid fa-map"> - Planos</i>
            <input name="Planos" placeholder="planos" type="file" id="planos" required>
        </label>

        <label>
            <i class="fa-solid fa-address-card"> - Informe de dominio (Reg. Inmob.)</i>
            <input name="RegInmob" placeholder="Registro Inmobiliario" type="file" id="RegInmob" required>
        </label>

        <label>
            <i class="fa-solid fa-gavel"> - Libre deuda (Tribu. de faltas)</i>
            <input name="TibunalDeFalta" placeholder="Tribunal de falta" type="file" id="TibunalDeFalta" required>
        </label>

        <label>
            <i class="fa-solid fa-square-parking"> - Permiso Provisorio</i>
            <input name="Permiso" placeholder="permiso" type="file" id="permiso" required>
        </label>


        <button id="button">Enviar</button>
        <br />
        <br />
        <a href="@Url.Action("Index", "ClienteProfesional")" class="link">Volver</a>
    </form>

    
    
    


    <script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
</body>
</html>
